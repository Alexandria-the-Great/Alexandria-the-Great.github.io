.lnav {
  position:fixed; // keep fixed to window
  float: left;
  padding: 15% 5% 5% 5%;;
  top: 0; left: 0; // position to top left of window
  height:100%; width:50px; //set dimensions
  transition: width ease .5s; // fluid transition when resizing
  /*margin-top: 500px;*/
  }
  
  .lnav ul li{
	  list-style: none;
  } 




.width ul li {
	list-style: none;
}




.alignleft {
	float: left;
	font-family:"channel-webfont";
	color: white;
	margin-top: 6%;
	font-size: 170%;
}
.alignright {
	float: right;
	font-family: "raleway-light-webfont";
	color: white;
	margin-top: 8%;
}

div.hr {
	float: right;
	margin-top: -1%;
	background: white no-repeat scroll center;
	width: 100%;
	height: 1px;
	border-width: 0;
	color: white;
	background-color: white;
	
}

.text {
	float: right;
	text-align: left;
	font-family: "raleway-regular-webfont";
	color: white;
	margin-top: 5%;
	margin-right: 10%;
	margin-left: 10%;
	
}






