#background {
    width: 100%; 
    height: 100%; 
    position: fixed; 
    left: 0px; 
    top: 0px; 
    z-index: -1; /* Ensure div tag stays behind content; -999 might work, too. */
}

* {
   margin: 0px;
   padding: 0px;
}
.stretch {
    width:100%;
    height:100%;
}

html, body{
    margin:0px;
    padding:0px;
    background-color: white;
}

.nav {
	height: 30px;
	background: white;
	/*text-align: center; */
	color: #f9c0c4;
	margin: 0;
	  position: fixed;
	  top: 0;
	  left: 0;
    right: 0;
	z-index: 9999;
	width: 100%; 
}


.nav ul {
	margin: 0;
	padding: 0;
}

.nav ul li {
	list-style: none;
}

.nav ul li a {
	text-decoration: none;
	float: left;
	display: block;
	padding: 6px 60% 0px 3%;
	color: #f9c0c4;

}


.nav ul li.selected a {
	color: #ed1c7e; 
}



.nav ul li span {
	float: right;
}


.nav ul li a:hover {
	color: #ed1c7e;
	-webkit-transition: all 0.3s ease-in;
   -moz-transition: all 0.3s ease-in;
   -o-transition: all 0.3s ease-in;
   transition: all 0.3s ease-in;
	-webkit-transition: all 0.3s ease-out;
   -moz-transition: all 0.3s ease-out;
   -o-transition: all 0.3s ease-out;
   transition: all 0.3s ease-out;
}


p {
	font-family:"raleway-semibold-webfont";
}




.clear{
 clear: both;
}



